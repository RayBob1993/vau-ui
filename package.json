{
  "name": "vau-ui",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://github.com/RayBob1993/vau-ui.git",
  "description": "Компонент для создания баннеров с Canvas и Fabric.js",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vite",
    "prepare": "husky",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint:js": "eslint --fix",
    "lint": "pnpm lint:js"
  },
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": "22.17.1"
  },
  "peerDependencies": {
    "vue": "3.5.22"
  },
  "dependencies": {
    "@vau/ui": "workspace:*",
    "@vau/core": "workspace:*",
    "@vau/themes": "workspace:*"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@stylistic/eslint-plugin": "5.4.0",
    "@types/node": "24.8.1",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "@vue/test-utils": "2.4.6",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/tsconfig": "0.8.1",
    "eslint": "9.37.0",
    "eslint-plugin-unused-imports": "4.2.0",
    "eslint-plugin-vue": "10.5.1",
    "eslint-plugin-import": "2.32.0",
    "happy-dom": "20.0.5",
    "husky": "9.1.7",
    "lint-staged": "16.2.4",
    "typescript": "5.9.3",
    "vite": "7.1.10",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.1.1",
    "sass-embedded": "1.93.2"
  }
