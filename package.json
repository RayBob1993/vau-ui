    "husky": "9.1.7",
    "lint-staged": "16.1.6",
    "typescript": "5.9.2",
    "vite": "7.1.4",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.0.6"
  }
